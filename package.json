{
  "name": "forge",
  "productName": "forge",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "homepage": "./",
  "watch": {
    "s": {
      "patterns": [
        "src/server",
        "src/util",
        "src/db"
      ],
      "extensions": "ts,tsx"
    }
  },
  "scripts": {
    "s": "electron-forge start",
    "dev": "npm-watch s",
    "d": "cmd/dev.sh",
    "n": "nodemon --exec electron-forge start",
    "p": "electron-forge package",
    "m": "electron-forge make",
    "b": "run-s p m",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "Matt Mannion",
    "email": "mattpmannion@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack/webpack.main.config.js",
            "renderer": {
              "nodeIntergration": false,
              "config": "./webpack/webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/client/base/index.html",
                  "js": "./src/client/router.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ],
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "forge"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "6.0.0-beta.63",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/sqlite3": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "css-loader": "^6.0.0",
    "electron": "16.0.7",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "node-loader": "^2.0.0",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "dotenv": "^14.3.2",
    "electron-squirrel-startup": "^1.0.0",
    "lowdb": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "5",
    "webpack": "^5.67.0"
  }
}
